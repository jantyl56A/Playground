#ORDER AIRLINES BY NUMBER OF PASSENGERS FLOWN

SELECT SUM(PASSENGER_COUNT) AS TOTAL_PASSENGERS 
FROM FLIGHTS a
LEFT JOIN PLANES b
ON a.TAIL_NUMBER = b.TAIL_NUMBER
WHERE AIRLINE IN ('SKY', 'ALN','GLB')
GROUP BY AIRLINE
ORDER BY COUNT(AIRLINE) DESC;


ORDER AIRLINES BY LARGEST AIRMILE LIABILITY

SELECT AIRMILES, COUNT(*) AS NUM_MILES
FROM PASSENGERS 
GROUP BY AIRMILES_CODE
ORDER BY NUM_MILES DESC


